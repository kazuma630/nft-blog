{
  "extends": [
    "next/core-web-vitals",
    "prettier" // 他の設定の上書きを行うために、必ず最後に配置
  ],
  "rules": {
    // importの順番をルール化
    // 参考：https://github.com/benmosher/eslint-plugin-import/blob/master/docs/rules/order.md
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
  }
}
